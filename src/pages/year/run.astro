---
import RunYearSport from "./../../components/RunYearSport.vue";
import ThemeToggle from "./../../components/ThemeToggle.vue";
import "./../../styles/stats.css";
---

<!DOCTYPE html>
<html lang="en" data-theme="dark">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
  <link href="/192.png" rel="icon" sizes="192x192" type="image/png">
  <title>Strava Year in Sport 2025 - Running</title>
  <meta name="description" content="Strava Year in Sport 2025 - Top Sport of the Year 2025 (Running).">
  <link rel="canonical" href="https://fit.santhoshveer.com/year/run/">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Mozilla+Text:wght@200..700&display=swap" rel="stylesheet">
    <script>
      const t = localStorage.getItem("stats-theme");
      if (t) document.documentElement.setAttribute("data-theme", t);
    </script>
  </head>
  <body>
    <div class="is-flex is-justify-content-center mt-6">
      <ThemeToggle client:load />
    </div>
    <RunYearSport client:load />
  </body>
</html>